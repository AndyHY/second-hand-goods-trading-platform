/* pages/mine/mine.wxss */
page {
    height:100%;
    width:100%;
}
.contain {
    height:100%;
    width: 100%;
    display: flex;
    flex-direction: column;
    background-color: #edece8;
}
.userInform {
    display: flex;
    align-items: center;
    border-bottom: 1rpx solid lightgray;
    border-top: 1rpx solid lightgray
}
.icon {
    margin: 20rpx;
    height: 75rpx;
    width: 75rpx;
    border-radius: 50%;
    border: 5rpx solid gray
}
.nickname {
    display: inline-block;
    margin-left:20rpx;
}
.block {
    height:100rpx;
    display: flex;
    align-items: center;
    margin-top: 10rpx;
    margin-left: 15rpx;
    margin-right: 15rpx;
    background-color: #f5f5f5;
    padding-left: 10rpx;
    border-radius: 10rpx;
}